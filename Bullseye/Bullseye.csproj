<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Description>For describing and running targets and their dependencies.</Description>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <LangVersion>latest</LangVersion>
    <PackageIconUrl>https://raw.githubusercontent.com/adamralph/bullseye/master/assets/bullseye.png</PackageIconUrl>
    <PackageLicenseUrl>https://github.com/adamralph/bullseye/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/adamralph/bullseye</PackageProjectUrl>
    <PackageReleaseNotes>https://github.com/adamralph/bullseye/milestones?state=closed</PackageReleaseNotes>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <TargetFrameworks>netstandard1.3;netstandard2.0</TargetFrameworks>
    <VersionPrefix>1.1.0</VersionPrefix>
    <VersionSuffix>$(VERSION_SUFFIX)</VersionSuffix>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.CodeQuality.Analyzers" Version="2.6.1" PrivateAssets="All" />
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.0.0-beta-63127-02" PrivateAssets="All" />
  </ItemGroup>

</Project>
